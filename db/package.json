{
  "name": "db",
  "private": true,
  "scripts": {
    "migrate:create": "node-pg-migrate create --migrations-dir ./migrations",
    "migrate:up": "dotenv -e .env.local -- node-pg-migrate up --migrations-dir ./migrations",
    "migrate:down": "dotenv -e .env.local -- node-pg-migrate down --migrations-dir ./migrations",
    "seed": "dotenv -e .env.local -- node ./seeds/seed.cjs"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "node-pg-migrate": "^7.6.0"
  },
  "dependencies": {
    "pg": "^8.12.0"
  }
}